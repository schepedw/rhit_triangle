<div class = 'container'>
  <div class = 'row'>
    <div class = 'col-xs-12 col-md-6 col-md-offset-3 registration-form'>
      <h2>Update Officers</h2>

      <% @alumni_officers.each do |officer| %>
        <div data-alumni-officer-id=<%=officer.id%>>
          <%= render 'show', officer: officer %>
        </div>
      <% end %>
      <br />
    </div>
  </div>
</div>
